{
  "name": "stackk-biomedical",
  "private": true,
  "scripts": {
    "android": "expo run:android",
    "auth:generate": "pnpm -F @stackk/auth generate",
    "build": "turbo run build",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo run clean",
    "db:push": "turbo -F @stackk/db push",
    "db:studio": "turbo -F @stackk/db studio",
    "dev": "turbo watch dev --continue",
    "dev:next": "turbo watch dev -F @stackk/nextjs...",
    "format": "turbo run format --continue -- --cache --cache-location .cache/.prettiercache",
    "format:fix": "turbo run format --continue -- --write --cache --cache-location .cache/.prettiercache",
    "postinstall": "pnpm lint:ws",
    "ios": "expo run:ios",
    "lint": "turbo run lint --continue -- --cache --cache-location .cache/.eslintcache",
    "lint:fix": "turbo run lint --continue -- --fix --cache --cache-location .cache/.eslintcache",
    "lint:ws": "pnpm dlx sherif@latest",
    "typecheck": "turbo run typecheck",
    "ui-add": "turbo run ui-add"
  },
  "prettier": "@stackk/prettier-config",
  "devDependencies": {
    "@stackk/prettier-config": "workspace:*",
    "@turbo/gen": "^2.5.4",
    "prettier": "catalog:",
    "turbo": "^2.5.4",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "engines": {
    "node": ">=22.14.0",
    "pnpm": ">=10.11.1"
  }
}
